{{#partial "content"}}
   <h1>Team:</h1>
   <p>Team Name:</p>
      <p>{{team.teamName}}</p>
      <h5>Team Description:</h5>
      <p> {{team.teamDescription}}</p>
      <h5>Team Member 1:</h5>
      <p> {{team.member1}}</p>
      <h5>Team Member 2:</h5>
      <p> {{team.member2}}</p>
      <h5>Team Member 3</h5>
      <p> {{team.member3}}</p>
      <h5>Team Member 4</h5>
      <p> {{team.member4}}</p>
      <p></p>
      <p></p>
   <h5>Created At:</h5>
   <p> {{ post.createdAt }}</p>
   <h5>Post status:</h5>
   <p></p>
   <p></p>
   <p></p>
   <p>
       {{#if post.published }}
           <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
       {{else}}
           <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
       {{/if}}
   </p>
{{/partial}}

{{> layout.hbs}}
